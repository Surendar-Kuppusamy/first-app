        "symfony/http-foundation": "^5.1",
        "catfan/medoo": "^1.7",
        "monolog/monolog": "^2.1",
        "colinmollenhour/credis": "^1.11",
        "rakit/validation": "^1.4",
        "aura/session": "^2.1",
        "vlucas/phpdotenv": "^5.2",
        "firebase/php-jwt": "^5.2",
